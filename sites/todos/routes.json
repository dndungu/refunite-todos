[
    {
        "matches": ["/"],
        "type" : "html",
        "stylesheet" : "index.xsl",
        "apps" : [
            {
                "app" : "oauth",
                "handler" : "avatar",
                "permissions" : ["public.permission"],
                "cache" : 3600
            }
        ],
        "permissions": ["public.permission"],
        "sync": true
    },
    {
        "matches": ["/dashboard"],
        "type" : "html",
        "stylesheet" : "dashboard.xsl",
        "apps" : [
            {
                "app" : "oauth",
                "handler" : "avatar",
                "permissions" : ["user.permission"],
                "cache" : 3600
            }
        ],
        "permissions": ["user.permission"],
        "sync": true
    },
    {
        "matches": ["/sign-out"],
        "type" : "html",
        "stylesheet" : "index.xsl",
        "apps" : [
            {
                "app" : "oauth",
                "handler" : "avatar",
                "permissions" : ["public.permission"],
                "cache" : 0
            },
            {
                "app" : "user",
                "handler" : "signout",
                "permissions" : ["public.permission"],
                "cache" : 0
            }
        ],
        "permissions": ["public.permission"],
        "sync": true
    },
    {
        "matches": ["/api/<model>/<_id>", "/api/<model>"],
        "type": "json",
        "apps" : [
            {
                "app": "generic",
                "handler": "rest",
                "permissions": ["user.permission"],
                "cache": 10
            }
        ],
        "permissions": ["user.permission"],
        "parameters": {"store": "public/settingss"},
        "sync": false
    },
    {
        "matches": ["/static/<path>"],
        "type": "json",
        "apps" : [
            {
                "app": "website",
                "handler": "static",
                "permissions": ["public.permission"],
                "cache": 3600
            }
        ],
        "permissions": ["public.permission"],
        "sync": true
    },
    {
        "matches": ["/oauth/connect/<network>/<next>"],
        "type": ["text"],
        "apps": [
            {
                "app": "oauth",
                "handler": "connect",
                "permissions": ["public.permission"],
                "cache": 0
            }
        ],
        "permissions": ["public.permission"],
        "sync": true
    },
    {
        "matches": ["/oauth/connect-twitter/<next>"],
        "type": ["text"],
        "apps": [
            {
                "app": "oauth",
                "handler": "connect-twitter",
                "permissions": ["public.permission"],
                "cache": 0
            }
        ],
        "permissions": ["public.permission"],
        "sync": true
    },
    {
        "matches": ["/oauth/callback/<network>/<next>"],
        "type": "text",
        "apps": [
            {
                "app": "oauth",
                "handler": "signup",
                "permissions": ["public.permission"],
                "cache": 0
            }
        ],
        "permissions": ["public.permission"],
        "parameters": {
            "redirect": "/dashboard"
        },
        "sync": true
    },
    {
        "matches": ["/oauth/callback-twitter/<next>"],
        "type": "text",
        "apps": [
            {
                "app": "oauth",
                "handler": "signup-twitter",
                "permissions": ["public.permission"],
                "cache": 0
            }
        ],
        "permissions": ["public.permission"],
        "parameters": {
            "redirect": "/dashboard"
        },
        "sync": true
    }
]
